<%- include("../partials/header") %>

<form method="POST" action="/cohorts">
    <div class="form-row">
        <div class="form-group col-md-6">
           <label for="logo_url">Logo Url</label>
           <input name="logo_url" type="url" class="form-control" id="logo_url" placeholder="e.g. http://www.example.com/some.png"/>
        </div>

        <div class="form-group col-md-6">
           <label for="text">Name</label>
           <input name="name" type="text" class="form-control" id="name" placeholder="Flying Funks"/>
        </div>

        <div class="form-group col-md-6">
           <label for="text">Members</label>
           <input name="members" type="text" class="form-control" id="members" placeholder="Jon Snow,Daenerys">
        </div>

    </div>

    <button type="submit" class="btn btn-outline-success">Submit</button>
    
  </form>

<%- include("../partials/footer") %>